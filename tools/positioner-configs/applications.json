{
    "apps": [
        {
            "command": "/usr/local/bin/rfmeas --config /home/rfmeas/project/settings.json --runtime /home/rfmeas/project/runtime.json",
            "name": "rfmeas",
            "restart": "always",
            "systemd_bind" : "rfmeas"
        },
        {
            "command": "/usr/local/bin/crowker --udp 10009",
            "name": "crowker",
            "restart": "always"
        },
        {
            "command": "/usr/local/bin/dataproxy",
            "name": "dataproxy",
            "restart": "always",
            "systemd_bind" : "dataproxy"
        },
        {
            "command": "/usr/local/bin/ModbusRemoteControl --hmi_ip 192.168.2.253",
            "name": "ModbusRemoteControl",
            "restart": "always"
        },
        {
            "command": "/usr/bin/reproxy --static.enabled --static.rule=*,/control/(.*),http://127.0.0.1:9000/$1 --assets.location=/home/rfmeas/web/ -l 0.0.0.0:8080",
            "name": "reproxy",
            "restart": "always"
        },
        {
            "command": "/usr/bin/uredir :21845 192.168.0.253:21845 -n",
            "name": "uredir",
            "restart": "always"
        },
        {
            "command": "/usr/local/bin/bshell",
            "name": "bshell",
            "restart": "always"
        }
    ]
}